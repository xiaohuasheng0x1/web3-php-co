{
    "name": "xiaohuasheng0x1/web3-php-co",
    "version": "1.0.0",
    "description": "Ethereum web3 interface.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "sc0Vu",
            "email": "alk03073135@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-swoole": "*",
        "ext-mbstring": "*",
        "swlib/saber": "^1.0",
        "swoole/ide-helper": "^5.0",
        "kornrunner/keccak": "~1.0",
        "phpseclib/phpseclib": "~2.0.30"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0|~9.0"
    },
    "autoload": {
        "psr-4": {
            "Web3\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "test/"
        }
    },
    "config": {
        "sort-packages": true
    },

    "minimum-stability": "dev",
    "prefer-stable": true
}
